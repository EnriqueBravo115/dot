font_family      Iosevka Nerd Font
font_size 15
shell_integration  no-cursor
enable_audio_bell no
cursor_blink_interval 0
#background_opacity 0.90

# Reset existing mappings
map ctrl+alt+tab        next_tab
map ctrl+alt+shift+tab previous_tab
# Send Ctrl Tab and Ctrl + Shift + Tab for Tmux navigation
map ctrl+tab send_text all \x1b[27;5;9~
map ctrl+shift+tab send_text all \x1b[27;6;9~

foreground                      #bbc2cf
background                      #002b36
selection_foreground            #bbc2cf
selection_background            #ae81ff

cursor #ff335c
cursor_text_color               #282c34

active_border_color     #46D9FF
inactive_border_color   #3f444a

active_tab_foreground   #282c34
active_tab_background   #DFDFDF
inactive_tab_foreground #3f444a
inactive_tab_background #5B6268

color0 #2a2e38
color8 #3f444a

color1 #ff6c6b
color9 #ff6655

color2  #c678dd
color10 #99bb66

color3  #ECBE7B
color11 #ECBE7B

color4  #51afef
color12 #51afef

color5  #c678dd
color13 #c678dd

color6  #46D9FF
color14 #46D9FF

color7  #DFDFDF
color15 #bbc2cf
